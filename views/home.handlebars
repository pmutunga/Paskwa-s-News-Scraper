{{> nav/homenav}}
<div class="row heading">
    <div class="col">
        <div class="jumbotron text-center">
            <div class="overlay">
                <div>
                    <div class="background-image"></div>
                    <div class="caption">
                        <h1> Mongo Scraper</h1>
                        <p> New York Times Edition</p>
                    </div>
                </div>
            </div>
        </div>
    </div> {{!end of col}}
</div> {{!end of heading row}}
<div class="row results">
    <div class="col">
        <div class="container-fluid article-container" id="articles">
            {{#each Headline}}
            <div class="media">
                <img class="mr-3" src="..." alt="Generic placeholder image">
                <div class="media-body">
                    <p class="mt-0">{{this.title}}</p>
                    <p><a href="https://www.cnet.com{{this.link}}>">Read</a></p>
                </div>
            </div>
            {{/each}}
        </div>
    </div>{{!end of col}}
</div> {{!end of articles row}}
{{> footer/footer}}

